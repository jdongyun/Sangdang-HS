        </activity>
        <activity
            android:name="com.dongyun.sangdang.Schoolinfo"
            android:screenOrientation="portrait"
            android:label="@string/schoolinfo"
            android:parentActivityName="com.dongyun.sangdang.SubActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dongyun.sangdang.MainActivity" />
        </activity>
        <activity
            android:name="com.dongyun.sangdang.Schedule"
            android:screenOrientation="portrait"
            android:label="@string/schedule"
            android:parentActivityName="com.dongyun.sangdang.SubActivity"
            android:theme="@style/Theme.Sutaek_Material.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dongyun.sangdang.MainActivity" />
        </activity>
        <activity
            android:name="com.dongyun.sangdang.Schoolintro"
            android:screenOrientation="portrait"
            android:label="@string/schoolintro"
