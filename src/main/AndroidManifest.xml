        </activity>
        <activity
            android:name="com.dongyun.sangdang.Doc_Readme"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_doc__readme"
            android:parentActivityName="com.dongyun.sangdang.Appinfo" >
