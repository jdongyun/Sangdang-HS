<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dongyun.sangdang"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Sutaek_Material">

        <receiver android:name="BootReceiver"
            android:process=":remote"/>
        <activity
            android:name="com.dongyun.sangdang.MainActivity"
            android:screenOrientation="portrait"
            android:icon="@drawable/ic_launcher"
            android:theme="@style/Theme.Sutaek_Material.Translucent"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dongyun.sangdang.SubActivity"
            android:screenOrientation="portrait"
            android:label="@string/allmenu"
            android:parentActivityName="com.dongyun.sangdang.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dongyun.sangdang.MainActivity" />
        </activity>
        <activity
            android:name="com.dongyun.sangdang.Appinfo"
            android:screenOrientation="portrait"
            android:label="@string/info"
            android:parentActivityName="com.dongyun.sangdang.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dongyun.sangdang.MainActivity" />
        </activity>
        <activity
            android:name="com.dongyun.sangdang.Notices"
            android:screenOrientation="portrait"
            android:label="@string/notices"
            android:parentActivityName="com.dongyun.sangdang.SubActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dongyun.sangdang.MainActivity" />
        </activity>
        <activity
            android:name="com.dongyun.sangdang.SchoolEvent"
            android:screenOrientation="portrait"
            android:label="@string/schoolevent"
            android:parentActivityName="com.dongyun.sangdang.SubActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dongyun.sangdang.MainActivity" />
        </activity>
        <activity
            android:name="com.dongyun.sangdang.Meal"
            android:screenOrientation="portrait"
            android:label="@string/meal"
